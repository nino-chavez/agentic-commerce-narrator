  "functions": [
    {
      "id": "fn-hosp-ops-housekeeping-task",
      "name": "Housekeeping Task Management",
      "description": "AI-powered housekeeping orchestration managing room status tracking, task assignment optimization, quality inspections, and productivity analytics",
      "traditional_workflow": "Housekeeping supervisors manually assign rooms to attendants based on floor sections, without considering task complexity or attendant workload. Room status updates are communicated via walkie-talkie or paper slips with delays and errors. Attendants clean rooms in sequential order without prioritization of VIP or early check-ins. Quality inspections are random spot checks covering 10-15% of rooms. Productivity tracking is manual, based on room counts without considering room conditions (checkout vs. stay-over) or special requests.",
      "agentic_workflow": "Task assignment agents optimize room allocation to housekeeping staff based on room location, complexity (checkout vs. stay-over), attendant skills, and current workload. Priority agents dynamically adjust cleaning sequence based on arrival times, VIP status, and guest requests. Room status agents enable real-time updates via mobile devices, instantly informing front desk of room availability. Inspection agents use risk scoring to target quality checks on high-priority rooms and new staff. Productivity analytics agents track performance metrics in real-time, identifying efficiency opportunities and training needs.",
      "agent_architecture": {
        "orchestrator": {
          "name": "Housekeeping Operations Orchestrator",
          "role": "Manages end-to-end housekeeping operations from task assignment through quality assurance, optimizing efficiency and guest satisfaction",
          "responsibilities": [
            "Optimize task assignments across staff",
            "Prioritize rooms based on arrivals and VIP status",
            "Manage real-time room status tracking",
            "Coordinate quality inspections and performance tracking"
          ]
        },
        "super_agents": [
          {
            "name": "Task Assignment Agent",
            "role": "Optimally assigns cleaning tasks to housekeeping staff based on workload, skills, location, and room complexity",
            "capabilities": [
              "Workload balancing across attendants",
              "Location-based clustering to minimize travel",
              "Task complexity consideration (checkout vs. stay-over)",
              "Skill-based assignment for special requests"
            ]
          },
          {
            "name": "Priority Scheduling Agent",
            "role": "Dynamically prioritizes room cleaning based on arrivals, VIP status, early check-ins, and operational needs",
            "capabilities": [
              "Arrival-driven prioritization",
              "VIP room expediting",
              "Early check-in request handling",
              "Checkout time optimization"
            ]
          },
          {
            "name": "Room Status Agent",
            "role": "Tracks real-time room status (dirty, cleaning, clean, inspected) with mobile updates from housekeeping staff",
            "capabilities": [
              "Real-time status updates via mobile devices",
              "Automated front desk synchronization",
              "Status change timestamp tracking",
              "Exception flagging (long cleaning times, issues found)"
            ]
          },
          {
            "name": "Quality Inspection Agent",
            "role": "Manages inspection scheduling, checklist compliance, defect tracking, and quality trend analysis",
            "capabilities": [
              "Risk-based inspection targeting",
              "Digital inspection checklists",
              "Photo documentation of issues",
              "Quality trend analysis by attendant and room type"
            ]
          }
        ],
        "utility_agents": [
          {
            "name": "Productivity Analytics Agent",
            "role": "Tracks and analyzes housekeeping productivity, identifying efficiency opportunities"
          },
          {
            "name": "Linen & Supply Agent",
            "role": "Monitors linen and amenity inventory, triggering replenishment"
          }
        ]
      ],
      "hitl_gates": [
        {
          "gate_id": "gtg-quality-failures",
          "name": "Quality Inspection Failure",
          "trigger": "Room fails quality inspection with major defects or repeated issues",
          "human_role": "Housekeeping Supervisor",
          "decision_criteria": "Defect severity, re-clean requirements, staff coaching needs, guest impact",
          "auto_proceed_conditions": "Rooms passing inspection or minor defects auto-logged for follow-up",
          "escalation_path": "Executive Housekeeper for patterns of quality issues or serious defects"
        ],
        {
          "gate_id": "gtg-workload-imbalance",
          "name": "Extreme Workload Imbalance",
          "trigger": "AI assignment creates workload variance >30% between attendants or overtime risk",
          "human_role": "Housekeeping Supervisor",
          "decision_criteria": "Staffing constraints, union rules, individual capabilities, fairness considerations",
          "auto_proceed_conditions": "Balanced assignments within 15% variance and normal workload parameters",
          "escalation_path": "Executive Housekeeper for staffing shortage issues"
        }
      ],
      "data_sources": [
        {
          "source": "Property Management System",
          "data_elements": "Checkout list, arrival list, stay-over list, VIP flags, early check-in requests, room types",
          "integration_pattern": "Real-time API, morning batch updates"
        ],
        {
          "source": "Housekeeping Management System",
          "data_elements": "Task assignments, room status, attendant locations, productivity data, inspection results",
          "integration_pattern": "Real-time bidirectional sync"
        ],
        {
          "source": "Mobile Devices",
          "data_elements": "Room status updates, task completion, inspection photos, issue reporting, time tracking",
          "integration_pattern": "Mobile app with offline capability"
        ],
        {
          "source": "Staff Management",
          "data_elements": "Attendant schedules, skills, languages, performance ratings, training records",
          "integration_pattern": "Workforce management integration"
        ],
        {
          "source": "Guest Service Requests",
          "data_elements": "Special requests (extra amenities, hypoallergenic bedding), complaints, service requests",
          "integration_pattern": "Service request system integration"
        ],
        {
          "source": "Inventory Management",
          "data_elements": "Linen counts, amenity stock levels, cleaning supply inventory",
          "integration_pattern": "Inventory system, par level tracking"
        ],
        {
          "source": "Quality Management",
          "data_elements": "Inspection checklists, defect types, corrective actions, quality scores by attendant",
          "integration_pattern": "Quality tracking database"
        }
      ],
      "quantified_benefits": {
        "room_cleaning_efficiency": "12-15 minutes average → 10-12 minutes with optimized assignment and prioritization",
        "room_availability_speed": "30-60 minutes from clean to front desk notification → 2-5 minutes real-time",
        "labor_utilization": "60-70% productive time → 85-90% with optimized task assignment",
        "early_check_in_success": "40-50% → 75-85% with prioritized cleaning",
        "quality_inspection_coverage": "10-15% random → 30-40% risk-based targeted inspections",
        "guest_satisfaction": "80-85% housekeeping CSAT → 90-95% with faster, prioritized service"
      ],
      "transformation_guidance": {
        "quick_wins": [
          "Deploy mobile devices for real-time room status updates (Week 1-2)",
          "Implement basic task assignment with location clustering (Week 2-3)",
          "Enable arrival-based priority cleaning (Week 3-4)"
        ],
        "investment_required": "Medium - Housekeeping management software, mobile devices for staff, PMS integration",
        "time_to_value": "6-10 weeks for core automation, 12-16 weeks for advanced optimization and analytics",
        "prerequisites": [
          "Housekeeping management system or mobile solution",
          "PMS integration for arrival/departure data",
          "Mobile devices for housekeeping staff (smartphones or tablets)",
          "Wi-Fi coverage throughout property",
          "Digital inspection checklist development",
          "Staff training on mobile tools and real-time communication",
          "Change management for shift from paper-based to digital processes"
        ]
      }
    },
    {
      "id": "fn-hosp-ops-maintenance-automation",
      "name": "Maintenance Work Order Automation",
      "description": "AI-powered maintenance system managing preventive maintenance scheduling, repair requests, vendor coordination, and asset lifecycle tracking",
      "traditional_workflow": "Maintenance requests come via phone calls, paper slips, or radio messages with inconsistent details. Work orders are manually created and assigned based on technician availability without considering skills, parts availability, or priority. Preventive maintenance is scheduled on fixed calendars without considering actual equipment usage or condition. Vendor coordination for outsourced work requires phone calls and email follow-ups. Asset histories are incomplete, and recurring issues go undetected due to lack of centralized tracking.",
      "agentic_workflow": "Work order agents automatically capture maintenance requests from multiple channels (guest app, staff reporting, IoT sensors) with structured details. Assignment agents optimize technician dispatch based on skills, location, workload, and parts availability. Predictive maintenance agents analyze equipment data to schedule maintenance based on actual condition rather than fixed intervals. Vendor coordination agents automate scheduling, scope communication, and completion verification for outsourced work. Analytics agents identify recurring issues and recommend root cause investigations or asset replacements.",
      "agent_architecture": {
        "orchestrator": {
          "name": "Maintenance Operations Orchestrator",
          "role": "Manages maintenance lifecycle from request through completion, coordinating technicians, vendors, parts, and asset management",
          "responsibilities": [
            "Route and prioritize work orders",
            "Optimize technician assignments",
            "Schedule preventive maintenance",
            "Coordinate vendor services and track asset lifecycle"
          ]
        ],
        "super_agents": [
          {
            "name": "Work Order Management Agent",
            "role": "Captures, categorizes, and routes maintenance requests with priority scoring and SLA tracking",
            "capabilities": [
              "Multi-channel request capture (app, staff, IoT)",
              "Automatic categorization and priority scoring",
              "SLA management by issue type",
              "Duplicate detection and consolidation"
            ]
          },
          {
            "name": "Technician Assignment Agent",
            "role": "Optimally assigns work orders to technicians based on skills, location, workload, and parts availability",
            "capabilities": [
              "Skill-based routing (HVAC, plumbing, electrical)",
              "Location-based assignment to minimize travel",
              "Workload balancing",
              "Parts availability checking before assignment"
            ]
          },
          {
            "name": "Predictive Maintenance Agent",
            "role": "Analyzes equipment data and usage patterns to schedule condition-based preventive maintenance",
            "capabilities": [
              "Usage-based maintenance scheduling",
              "Equipment health monitoring from IoT sensors",
              "Failure prediction based on historical patterns",
              "Maintenance window optimization to minimize guest impact"
            ]
          },
          {
            "name": "Vendor Coordination Agent",
            "role": "Manages outsourced maintenance through automated scheduling, communication, and completion verification",
            "capabilities": [
              "Automated vendor selection based on specialization",
              "Work scope communication and documentation",
              "Schedule coordination and confirmation",
              "Completion verification and quality tracking"
            ]
          }
        ],
        "utility_agents": [
          {
            "name": "Parts Inventory Agent",
            "role": "Tracks parts inventory, triggers reordering, links work orders to parts usage"
          },
          {
            "name": "Asset Lifecycle Agent",
            "role": "Maintains asset history, tracks total cost of ownership, recommends replacement timing"
          }
        ]
      ],
      "hitl_gates": [
        {
          "gate_id": "gtg-urgent-safety",
          "name": "Urgent Safety or Guest-Impacting Issue",
          "trigger": "Work order flagged as safety risk or seriously impacting guest experience",
          "human_role": "Maintenance Supervisor or Chief Engineer",
          "decision_criteria": "Safety severity, guest relocation needs, immediate response requirements, resource mobilization",
          "auto_proceed_conditions": "Routine maintenance and minor repairs assigned automatically",
          "escalation_path": "Duty Manager and General Manager for serious safety or property damage issues"
        ],
        {
          "gate_id": "gtg-major-expenditure",
          "name": "High-Cost Repair or Replacement",
          "trigger": "Repair estimate exceeds threshold ($5,000+) or asset replacement recommended",
          "human_role": "Chief Engineer",
          "decision_criteria": "Repair vs. replace analysis, budget availability, guest impact, timing considerations",
          "auto_proceed_conditions": "Routine repairs and preventive maintenance within budget parameters",
          "escalation_path": "General Manager or Regional Operations for capital expenditure approvals >$25K"
        }
      ],
      "data_sources": [
        {
          "source": "Computerized Maintenance Management System (CMMS)",
          "data_elements": "Work orders, preventive maintenance schedules, asset registry, maintenance history, technician assignments",
          "integration_pattern": "Real-time bidirectional API"
        ],
        {
          "source": "IoT Sensors & Building Management System",
          "data_elements": "Equipment status, temperature sensors, vibration analysis, filter pressure, runtime hours",
          "integration_pattern": "Real-time telemetry, threshold alerting"
        ],
        {
          "source": "Guest Service Requests",
          "data_elements": "Guest-reported maintenance issues (HVAC, plumbing, electrical, in-room issues)",
          "integration_pattern": "Service request system integration"
        ],
        {
          "source": "Mobile Maintenance App",
          "data_elements": "Work order updates, technician locations, completion photos, time tracking, parts used",
          "integration_pattern": "Mobile app with offline capability"
        ],
        {
          "source": "Staff Management",
          "data_elements": "Technician schedules, skills/certifications, performance metrics, training records",
          "integration_pattern": "Workforce management system"
        ],
        {
          "source": "Parts Inventory System",
          "data_elements": "Parts stock levels, par levels, usage rates, reorder points, supplier information",
          "integration_pattern": "Inventory management system"
        ],
        {
          "source": "Vendor Management",
          "data_elements": "Vendor contacts, specializations, contract terms, performance history, response times",
          "integration_pattern": "Vendor database, contract management"
        ],
        {
          "source": "Asset Management",
          "data_elements": "Equipment registry, purchase dates, warranty information, depreciation, lifecycle stage",
          "integration_pattern": "Asset tracking system"
        }
      ],
      "quantified_benefits": {
        "response_time": "30-60 minutes for urgent issues → 10-20 minutes with automated dispatch",
        "technician_efficiency": "60-70% wrench time → 80-85% with optimized assignments and mobile tools",
        "preventive_maintenance_effectiveness": "30-40% time-based → 60-70% condition-based reduces failures",
        "guest_impact_reduction": "40-50% reduction in guest-facing equipment failures",
        "maintenance_cost": "10-15% reduction through predictive maintenance and lifecycle optimization",
        "asset_uptime": "90-92% → 96-98% with proactive monitoring and predictive interventions"
      ],
      "transformation_guidance": {
        "quick_wins": [
          "Implement digital work order capture from staff and guest apps (Week 1-2)",
          "Deploy mobile maintenance app for technician updates (Week 2-3)",
          "Enable basic skill-based work order assignment (Week 3-4)"
        ],
        "investment_required": "Medium-High - CMMS or upgrade, mobile devices, IoT sensors for critical equipment, integration development",
        "time_to_value": "8-12 weeks for core work order automation, 6-12 months for predictive maintenance as data accumulates",
        "prerequisites": [
          "CMMS with API capabilities or modern replacement",
          "Comprehensive asset inventory and history",
          "Mobile devices for maintenance technicians",
          "Critical equipment identification for IoT monitoring",
          "Vendor database with contact and performance data",
          "Parts inventory tracking system",
          "Staff training on digital work order management",
          "Integration with guest service request system"
        ]
      }
    },
    {
      "id": "fn-hosp-ops-inspection-quality",
      "name": "Property Inspection & Quality Control",
      "description": "AI-powered inspection system managing automated inspection schedules, digital checklists, photo documentation, compliance tracking, and quality trend analysis",
      "traditional_workflow": "Property inspections are conducted on ad-hoc schedules using paper checklists that vary by inspector. Results are inconsistent due to subjective interpretations and checklist variations. Issues found are communicated verbally or via email, often without photo documentation. There is no centralized tracking of recurring issues or trend analysis across time periods. Follow-up on corrective actions is manual and inconsistent. Brand standard audits rely on infrequent external visits without continuous compliance monitoring.",
      "agentic_workflow": "Inspection scheduling agents create automated inspection cadences for all property areas (guest rooms, public spaces, BOH, exterior) with risk-based frequency. Digital checklist agents guide inspectors through standardized criteria with photo requirements and scoring guidance. Issue tracking agents automatically route defects to responsible departments with SLA tracking. Analytics agents identify recurring issues, trend patterns, and areas needing additional focus. Compliance agents monitor brand standard adherence continuously, flagging gaps before official audits. Predictive agents recommend proactive maintenance based on inspection trend data.",
      "agent_architecture": {
        "orchestrator": {
          "name": "Quality Inspection Orchestrator",
          "role": "Manages comprehensive property inspection program, coordinating schedules, compliance tracking, issue resolution, and continuous improvement",
          "responsibilities": [
            "Schedule inspections across all property areas",
            "Ensure checklist compliance and documentation",
            "Track issue resolution and corrective actions",
            "Analyze trends and drive quality improvements"
          ]
        ],
        "super_agents": [
          {
            "name": "Inspection Scheduling Agent",
            "role": "Creates and manages inspection schedules based on risk, compliance requirements, and historical findings",
            "capabilities": [
              "Risk-based inspection frequency (high-traffic vs. low-traffic areas)",
              "Compliance-driven scheduling (brand standards, health department)",
              "Inspector assignment based on expertise",
              "Schedule optimization to minimize operational disruption"
            ]
          },
          {
            "name": "Digital Checklist Agent",
            "role": "Guides inspectors through standardized checklists with photo requirements and consistent scoring",
            "capabilities": [
              "Standardized inspection criteria by area type",
              "Conditional questions based on responses",
              "Photo documentation requirements",
              "Offline capability with sync when connected"
            ]
          },
          {
            "name": "Issue Resolution Agent",
            "role": "Routes inspection findings to responsible departments, tracks corrective actions, and monitors completion",
            "capabilities": [
              "Automatic routing to maintenance, housekeeping, F&B, etc.",
              "SLA tracking by issue severity",
              "Corrective action verification",
              "Re-inspection triggering for major issues"
            ]
          },
          {
            "name": "Quality Analytics Agent",
            "role": "Analyzes inspection data to identify trends, recurring issues, and improvement opportunities",
            "capabilities": [
              "Trend analysis by area, time period, inspector",
              "Recurring issue pattern recognition",
              "Departmental performance scoring",
              "Predictive maintenance recommendations"
            ]
          }
        ],
        "utility_agents": [
          {
            "name": "Brand Compliance Agent",
            "role": "Monitors adherence to brand standards and flags gaps before official audits"
          },
          {
            "name": "Reporting Agent",
            "role": "Generates inspection summary reports for management and ownership"
          }
        ]
      ],
      "hitl_gates": [
        {
          "gate_id": "gtg-critical-defects",
          "name": "Critical Safety or Brand Violation",
          "trigger": "Inspection identifies critical safety issue or major brand standard violation",
          "human_role": "Operations Manager or General Manager",
          "decision_criteria": "Guest safety risk, brand compliance impact, immediate remediation needs, guest experience impact",
          "auto_proceed_conditions": "Minor defects auto-routed to departments with standard SLAs",
          "escalation_path": "General Manager for serious safety issues; Brand Leadership for major brand violations"
        ],
        {
          "gate_id": "gtg-recurring-failures",
          "name": "Recurring Quality Failures",
          "trigger": "Analytics agent identifies same issues failing repeatedly in area or by department",
          "human_role": "Department Head (Housekeeping, Maintenance, F&B)",
          "decision_criteria": "Root cause investigation needs, training requirements, process changes, accountability",
          "auto_proceed_conditions": "Isolated issues without patterns of recurrence",
          "escalation_path": "Operations Manager for systemic quality failures requiring process overhaul"
        }
      ],
      "data_sources": [
        {
          "source": "Inspection Management System",
          "data_elements": "Inspection schedules, checklist results, photos, scores, issue tracking, corrective actions",
          "integration_pattern": "Centralized inspection database"
        ],
        {
          "source": "Mobile Inspection App",
          "data_elements": "Real-time inspection data entry, photo capture, offline sync, GPS location tagging",
          "integration_pattern": "Mobile app with cloud sync"
        ],
        {
          "source": "Work Order Systems",
          "data_elements": "Maintenance and housekeeping work orders generated from inspection findings",
          "integration_pattern": "Bidirectional integration for issue creation and resolution tracking"
        ],
        {
          "source": "Brand Standards Database",
          "data_elements": "Brand compliance checklists, scoring criteria, audit history, certification requirements",
          "integration_pattern": "Brand system integration or manual maintenance"
        ],
        {
          "source": "Property Management System",
          "data_elements": "Room inventory, public space listings, F&B outlets, amenity locations",
          "integration_pattern": "Asset registry for inspection coverage"
        ],
        {
          "source": "Guest Feedback",
          "data_elements": "Quality-related complaints and compliments, survey responses, review mentions",
          "integration_pattern": "Feedback correlation with inspection findings"
        ],
        {
          "source": "Historical Inspection Data",
          "data_elements": "3-5 years of inspection results for trend analysis and predictive modeling",
          "integration_pattern": "Data warehouse, analytics platform"
        }
      ],
      "quantified_benefits": {
        "inspection_consistency": "60-70% consistency with paper checklists → 95%+ with digital standardization",
        "issue_documentation": "40-50% of issues with photos → 90%+ with required photo capture",
        "corrective_action_completion": "70-75% → 95%+ with automated tracking and SLA management",
        "brand_audit_scores": "5-10 point improvement in brand audit scores with continuous compliance monitoring",
        "proactive_issue_resolution": "60-70% reactive → 50-60% proactive based on trend identification",
        "inspection_efficiency": "20-30 minutes per room/area → 15-20 minutes with streamlined digital checklists"
      ],
      "transformation_guidance": {
        "quick_wins": [
          "Deploy digital inspection checklists on tablets for housekeeping inspections (Week 1-2)",
          "Implement photo documentation requirements for all issues (Week 2)",
          "Enable automated work order creation from inspection findings (Week 3-4)"
        ],
        "investment_required": "Medium - Inspection management software, mobile devices (tablets) for inspectors, photo storage, integration",
        "time_to_value": "6-10 weeks for digital inspection deployment, 12-16 weeks for analytics and trend identification",
        "prerequisites": [
          "Inspection management platform or CMMS with inspection module",
          "Mobile devices (tablets or smartphones) for inspectors",
          "Standardized inspection checklists and scoring criteria",
          "Brand standards documentation (if applicable)",
          "Integration with work order systems",
          "Photo storage infrastructure (cloud or local)",
          "Inspector training on digital tools and consistent scoring",
          "Management commitment to data-driven quality improvement"
        ]
      }
    },
    {
      "id": "fn-hosp-ops-energy-management",
      "name": "Energy Management & Sustainability",
      "description": "AI-powered energy optimization system managing HVAC, lighting, and utility consumption with occupancy-based controls, demand response, and sustainability reporting",
      "traditional_workflow": "HVAC systems run on fixed schedules without considering actual occupancy or weather conditions. Lights in public areas and back-of-house run continuously or on timers regardless of usage. Guest rooms are heated/cooled even when unoccupied between checkout and next arrival. Energy consumption is monitored monthly via utility bills without granular insights or real-time optimization. Sustainability initiatives are manual efforts without data-driven targeting. Peak demand charges hit during high-occupancy periods without load management strategies.",
      "agentic_workflow": "Occupancy agents integrate with PMS and mobile key systems to automatically adjust HVAC in vacant rooms, reducing energy waste while maintaining comfort for occupied spaces. Smart HVAC agents use weather forecasts and building thermal models to pre-condition spaces efficiently. Lighting agents use occupancy sensors and daylight harvesting to optimize illumination. Demand response agents manage peak load by coordinating equipment cycling and deferring non-critical loads. Analytics agents identify energy waste patterns and recommend targeted efficiency improvements. Sustainability reporting agents track carbon footprint and support ESG disclosure requirements.",
      "agent_architecture": {
        "orchestrator": {
          "name": "Energy Management Orchestrator",
          "role": "Manages building-wide energy optimization, coordinating HVAC, lighting, equipment controls, and sustainability tracking",
          "responsibilities": [
            "Optimize energy consumption across all systems",
            "Balance comfort with efficiency",
            "Manage demand response and peak load reduction",
            "Track and report sustainability metrics"
          ]
        ],
        "super_agents": [
          {
            "name": "Smart HVAC Agent",
            "role": "Optimizes heating and cooling based on occupancy, weather, and comfort requirements",
            "capabilities": [
              "Occupancy-based setpoint adjustments",
              "Weather-predictive pre-conditioning",
              "Zone-level optimization",
              "Checkout-to-arrival energy saving mode"
            ]
          },
          {
            "name": "Intelligent Lighting Agent",
            "role": "Manages lighting based on occupancy, daylight availability, and operational schedules",
            "capabilities": [
              "Occupancy sensor integration",
              "Daylight harvesting with dimming controls",
              "Schedule-based automation for public spaces",
              "Emergency and safety lighting override"
            ]
          },
          {
            "name": "Demand Response Agent",
            "role": "Manages peak demand reduction through load coordination and deferral strategies",
            "capabilities": [
              "Peak demand prediction",
              "Equipment cycling coordination",
              "Non-critical load deferral (e.g., laundry, water heating)",
              "Utility demand response program participation"
            ]
          },
          {
            "name": "Energy Analytics Agent",
            "role": "Monitors consumption patterns, identifies waste, and recommends efficiency improvements",
            "capabilities": [
              "Real-time consumption monitoring by system",
              "Anomaly detection (equipment malfunction, waste)",
              "Efficiency opportunity identification",
              "ROI modeling for energy projects"
            ]
          }
        ],
        "utility_agents": [
          {
            "name": "Sustainability Reporting Agent",
            "role": "Tracks carbon footprint, renewable energy usage, and ESG metrics"
          },
          {
            "name": "Guest Comfort Agent",
            "role": "Monitors and responds to guest temperature complaints, overriding efficiency for satisfaction"
          }
        ]
      ],
      "hitl_gates": [
        {
          "gate_id": "gtg-comfort-complaints",
          "name": "Guest Comfort Complaint",
          "trigger": "Guest reports room too hot, too cold, or requests temperature override beyond system parameters",
          "human_role": "Engineering or Guest Services",
          "decision_criteria": "Guest comfort priority, system capability, equipment status, efficiency impact",
          "auto_proceed_conditions": "Automatic adjustments within normal comfort ranges",
          "escalation_path": "Chief Engineer for equipment issues; GM for patterns indicating system inadequacy"
        ],
        {
          "gate_id": "gtg-major-equipment-changes",
          "name": "Major Equipment Control Strategy",
          "trigger": "AI recommends significant changes to HVAC schedules or equipment operation affecting large areas",
          "human_role": "Chief Engineer",
          "decision_criteria": "Impact on guest comfort, operational risk, energy savings magnitude, equipment lifespan",
          "auto_proceed_conditions": "Minor optimizations within tested parameters",
          "escalation_path": "General Manager for strategies with potential guest experience risk"
        }
      ],
      "data_sources": [
        {
          "source": "Building Management System (BMS)",
          "data_elements": "HVAC status, temperature sensors, equipment runtime, chiller/boiler performance, airflow",
          "integration_pattern": "BACnet, Modbus, or proprietary protocols; real-time telemetry"
        ],
        {
          "source": "Property Management System",
          "data_elements": "Room occupancy, checkout/arrival times, forecasted occupancy, guest temperature preferences",
          "integration_pattern": "API integration for occupancy-based control"
        ],
        {
          "source": "Mobile Key & IoT",
          "data_elements": "Room entry/exit events, real-time occupancy detection, DND status",
          "integration_pattern": "Event-driven triggers for HVAC adjustments"
        ],
        {
          "source": "Utility Meters",
          "data_elements": "Electric, gas, water consumption by interval (15-min or hourly), peak demand, power factor",
          "integration_pattern": "Smart meter integration, utility data feeds"
        ],
        {
          "source": "Weather Services",
          "data_elements": "Temperature forecasts, humidity, solar radiation, wind for predictive HVAC control",
